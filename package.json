{
  "name": "express-starter-kit",
  "version": "0.1.1",
  "main": "bin/start.js",
  "engines": {
    "node": ">=4.2 <6",
    "npm": ">=3.3"
  },
  "scripts": {
    "start": "node bin/start.js",
    "lint": "eslint bin src test",
    "debug": "DEBUG=app:* npm start",
    "clean": "rimraf docs coverage",
    "check": "npm run lint && npm run test",
    "test": "rimraf coverage && NODE_ENV=test istanbul cover node_modules/.bin/_mocha -- --reporter dot test/",
    "docs": "rimraf docs && jsdoc -c .jsdocconf -R README.md"
  },
  "dependencies": {
    "bluebird": "^3.1.1",
    "config": "^1.17.1",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "express-winston": "^1.2.0",
    "lodash": "^3.10.1",
    "swagger-jsdoc": "Cloudoki/swagger-jsdoc",
    "swagger-tools": "^0.9.11",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^1.0.0",
    "istanbul": "^0.4.2",
    "jsdoc": "^3.4.0",
    "mocha": "^2.3.4",
    "mocha-eslint": "^1.0.0",
    "rimraf": "^2.5.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.0.0"
  }
}
